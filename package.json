{
  "name": "profile-dashboard",
  "version": "1.0.0",
  "description": "A guest-book/dashboard of profiles",
  "main": "index.js",
  "author": "Megan Weijiang",
  "license": "MIT",
  "scripts": {
    "build": "babel . --ignore node_modules,build --out-dir build",
    "start:server": "cd backend && nodemon server.js --exec babel-node --presets es2015,stage-0",
    "start:client": "cd client && yarn start",
    "start:dev": "concurrently \"yarn start:server\" \"yarn start:client\"",
    "start:prod": "node build/server.js",
    "start": "npm run build",
    "heroku-postbuild": "yarn build"
  },
  "engines": {
    "node": "6.10.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.11.0",
    "concurrently": "^3.6.1",
    "express": "^4.16.3",
    "mongoose": "^5.2.9",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-markdown": "^3.4.1",
    "whatwg-fetch": "^2.0.4"
  }
}
